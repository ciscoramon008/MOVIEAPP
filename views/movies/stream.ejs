<%- include("../partials/header") %>

<div class="container my-5">
    <div class="row">
        <div class="col col-md-3">
            <div class="list-group">
                <a class="list-group-item list-group-item-action" href="/movies/<%= movie._id %>">ABOUT</a>
                <a class="list-group-item list-group-item-action" href="/movies/<%= movie._id %>/reviews">REVIEWS</a>
                <a class="list-group-item list-group-item-action" href="#">FULL-PLOT</a>
                <a class="list-group-item list-group-item-action" href="/movies/<%= movie._id %>/cast">CAST</a>
                <a class="list-group-item list-group-item-action" href="/movies/<%= movie._id %>/boxoffice">BOX OFFICE</a>
                <a class="list-group-item list-group-item-action active" href="/movies/<%= movie._id %>/stream">WHERE TO WATCH</a>
                <a class="list-group-item list-group-item-action" href="/movies/<%= movie._id %>/more">MORE LIKE THIS</a>
            </div>
        </div>
        <div class="col col-md-9 card p-3">
            <h1 class="display-4"><%= movie.name.toUpperCase() %></h1>
            <% if(movie.release < Date.now()){ %>
            <div class="d-flex justify-content-around align-items-center py-3">
                <a href="<%= movie.primeLink %>" id="aPrime" onclick="atChange('aPrime')" target="_blank"><img style="width: 150px;" src="https://www.brandsvectorlogo.com/content/uploads/images/t_amazon-prime-video.jpg"></a>
                <a href="<%= movie.netflixLink %>" id="aNetflix" onclick="atChange('aNetflix')" target="_blank"><img style="width: 120px;" src="https://assets.brand.microsites.netflix.io/assets/493f5bba-81a4-11e9-bf79-066b49664af6_cm_1440w.png?v=16"></a>
                <a href="<%= movie.hotstarLink %>" id="aHotstar" onclick="atChange('aHotstar')" target="_blank"><img style="width: 150px;" src="data:image/svg+xml;base64,"></a>
                <a href="<%= movie.youtubeLink %>" id="aYt" onclick="atChange('aYt')" target="_blank"><img style="width: 150px;" src="data:image/png;base64,"></a>
            </div>
            <div class="d-flex justify-content-around align-items-center py-3">
                <a href="#"><img style="width: 120px;" src="data:image/png;base64,"></a>
                <a href="#"><img style="width: 120px;" src="https://upload.wikimedia.org/wikipedia/commons/thumb/e/e4/Hulu_Logo.svg/1200px-Hulu_Logo.svg.png"></a>
                <a href="#"><img style="width: 120px;" src="https://images-na.ssl-images-amazon.com/images/I/51D75rDCQXL._SL1280_.jpg"></a>
                <a href="#"><img style="width: 150px;" src="https://play.hbogo.com/image/logo.png"></a>
            </div>
            <% } else { %>
                <p class="lead">MOVIE NOT RELEASED YET!</p>
            <% } %>
        </div>
    </div>
</div>

<script type="text/javascript">
    function atChange(element){
        dom = document.getElementById(element)
        if(dom.getAttribute('href') === ''){
            dom.setAttribute('href', '#')
            dom.removeAttribute('target')
        }
    }
</script>

<%- include("../partials/footer") %>